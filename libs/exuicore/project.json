{
  "name": "exuicore",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/exuicore/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/exuicore",
        "main": "libs/exuicore/src/index.ts",
        "tsConfig": "libs/exuicore/tsconfig.lib.json",
        "assets": ["libs/exuicore/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/exuicore/jest.config.ts"
      }
    }
  }
}
